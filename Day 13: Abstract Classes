class MyBook: public Book{
        
    private: int price;
    public:
    MyBook(string title, string author, int price): Book(title,author){
            this->price=price;
            this->title=title;
            this->author=author;
        }
            void display(){
            cout<<"Title: "<<title<<"\n";
            cout<<"Author: "<<author<<"\n";
            cout<<"Price: "<<price;
        }
    
    };
    
    
 
