#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n;
    cin>>n;
    for (int i=0;i<n;i++){

    string s1;
    cin>>s1;
    int l1=s1.length();

    for (int i1=0;i1<l1;i1++){
        if (i1%2==0){
            cout<<s1[i1];
        }
    }
    cout<<" ";
    for (int j=0;j<l1;j++){
        if (j%2==1){
            cout<<s1[j];
        }
    }
    cout<<"\n";
    
    }
    

    return 0;
}

